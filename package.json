{
  "name": "nest-typescript-starter",
  "private": true,
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "ducker:run": "docker run -p 3000:3000 onlinemenu.uz",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/jest/bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:run": "typeorm-ts-node-commonjs -d ./src/common/configs/database-migration.config.ts migration:run",
    "migration:create": "typeorm-ts-node-commonjs migration:create ./src/database/migrations/$npm_config_name",
    "migration:generate": "typeorm-ts-node-commonjs -d ./src/common/configs/database-migration.config.ts migration:generate ./src/database/migrations/$npm_config_name",
    "migration:revert": "typeorm-ts-node-commonjs -d ./src/common/configs/database-migration.config.ts migration:revert",
    "migration:show": "typeorm-ts-node-commonjs -d ./src/common/configs/database-migration.config.ts migration:show",
    "schema:sync": "typeorm-ts-node-commonjs -d ./src/common/configs/database-migration.config.ts schema:sync",
    "schema:log": "typeorm-ts-node-commonjs -d ./src/common/configs/database-migration.config.ts schema:log",
    "schema:drop": "typeorm-ts-node-commonjs -d ./src/common/configs/database-migration.config.ts schema:drop",
    "entity:create": "typeorm-ts-node-commonjs entity:create ./src/database/entities/$npm_config_name",
    "subscriber:create": "typeorm-ts-node-commonjs subscriber:create ./src/database/subscribers/$npm_config_name",
    "typeorm:query": "typeorm-ts-node-commonjs -d ./src/common/configs/database-migration.config.ts query \"$npm_config_query\"",
    "typeorm:cache:clear": "typeorm-ts-node-commonjs -d ./src/common/configs/database-migration.config.ts cache:clear",
    "typeorm:version": "typeorm-ts-node-commonjs version"
  },
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@nestjs/axios": "^3.1.2",
    "@nestjs/common": "^10.3.2",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.4",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.0.5",
    "@nestjs/platform-express": "^10.4.7",
    "@nestjs/swagger": "^8.1.0",
    "@nestjs/typeorm": "^10.0.2",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.5",
    "ioredis": "^5.4.1",
    "multer": "1.4.5-lts.1",
    "mysql2": "^3.11.4",
    "nestjs-telegraf": "^2.8.1",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "telegraf": "^4.16.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.2.0",
    "@nestjs/cli": "^10.4.5",
    "@nestjs/schematics": "^10.1.0",
    "@nestjs/testing": "^10.3.2",
    "@swc/cli": "^0.3.9",
    "@swc/core": "^1.4.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.17.6",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.20",
    "typescript": "^5.3.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "packageManager": "pnpm@9.10.0"
}
